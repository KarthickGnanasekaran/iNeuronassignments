function celtoFahren(tcelcius){
    let fahren = tcelcius * 9/5 +32;
   console.log(tcelcius+"째C is " + fahren + "째F"); 
}
celtoFahren(60);

function Fahrentocelcius(tfahren){
    let celcius = (tfahren-32) * 5/9;
    console.log(tfahren+"째F is " + celcius + "째C"); 
}
Fahrentocelcius(45);
